{
  "builds": [
    {
      "config": {
        "installCommand": "npm install",
        "buildCommand": "npm run build"
      }
      "src": "frontend/dist/**",
      "use": "@vercel/static"
    },
    { "src": "*.py", "use": "@vercel/python" },
    { "src": "backend/**.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/", "dest": "/frontend/dist/**" }
    { "src": "/.*", "dest": "/backend/app.js" },
  ]
}
